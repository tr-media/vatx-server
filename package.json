{
  "name": "vatx-server",
  "version": "1.2.2",
  "description": "vat:x server",
  "main": "dist/index.js",
  "scripts": {
    "nodemon": "nodemon dist/index.js",
    "postinstall": "typings install || exit 0",
    "clean": "rimraf dist node_modules typings",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "autobuild": "tsc --watch",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tr-media/vatx-server.git"
  },
  "author": {
    "name": "Tobias Reimann",
    "url": "https://www.tobias-reimann.com"
  },
  "homepage": "https://vatx.net",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/tr-media/vatx-server/issues"
  },
  "vatx": {
    "airport_rev": 1,
    "airline_rev": 1
  },
  "dependencies": {
    "command-line-args": "5.1.1",
    "compression": "1.7.4",
    "express": "4.16.4",
    "ipaddr.js": "1.9.0",
    "md5": "2.2.1",
    "moment": "2.24.0",
    "querystring": "0.2.0",
    "request": "2.88.0",
    "universal-analytics": "0.4.20"
  },
  "devDependencies": {
    "nodemon": "1.18.11",
    "rimraf": "2.6.3",
    "typescript": "3.4.4",
    "typings": "2.1.1"
  }
}
